aeroway=aerodrome {set name='${name}, аэродром' | 'аэродром' } [0x2f04 resolution 16]
aeroway=helipad {set name='${name}, вертолётная площадка' | 'вертолётная площадка' } [0x5904 resolution 18]
aeroway=terminal {set name='${name}, терминал' | 'терминал' } [0x2f04 resolution 20]

